@font-face {
	font-family: 'icomoon';
	src:url('/fonts/icomoon.eot');
	src:url('/fonts/icomoon.eot?#iefix') format('embedded-opentype'),
		url('/fonts/icomoon.svg#icomoon') format('svg'),
		url('/fonts/icomoon.woff') format('woff'),
		url('/fonts/icomoon.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

/* Use the following CSS code if you want to have a class per icon */
[class^="icon-"]:before,
[class*=" icon-"]:before {
	font-family: 'icomoon';
	font-style: normal;
	font-weight: normal;
	speak: none;
	-webkit-font-smoothing: antialiased;
}
.icon-github:before {
	content: "\21";
}
.icon-twitter:before {
	content: "\22";
}
.icon-arrow-up:before {
	content: "\23";
}
.icon-clock:before {
	content: "\24";
}
.icon-comments:before {
	content: "\25";
}
.icon-feed:before {
	content: "\26";
}
.icon-pencil:before {
	content: "\27";
}
.icon-link:before {
	content: "\28";
}
.icon-remove:before {
	content: "\29";
}
.icon-globe:before {
	content: "\2a";
}
.icon-stackoverflow:before {
	content: "\2b";
}
.icon-percolate:before {
	content: "\2c";
}